{
    "name": "almeidafranci/laravel-validator-extender",
    "description": "A laravel validator extender for commonly used rules.",
    "keywords": ["laravel", "laravel validator"],
    "require": {
        "php": "^7.2|^8.0",
        "illuminate/support": "~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0|^8.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Francinaldo Almeida",
            "email": "almeidafranci@users.noreply.github.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "AlmeidaFranci\\LaravelValidatorExtender\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AlmeidaFranci\\LaravelValidatorExtender\\ValidatorExtenderServiceProvider"
            ]
        }
    }
}
